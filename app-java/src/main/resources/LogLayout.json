{
    "mdc": {
        "$resolver": "mdc",
        "flatten": true
    },
    "message": {
        "$resolver": "message",
        "stringified": true
    },
    "thread_name": {
        "$resolver": "thread",
        "field": "name"
    },
    "logger_name": {
        "$resolver": "logger",
        "field": "name"
    },
    "level": {
        "$resolver": "level",
        "field": "name"
    },
    "exception_class": {
        "$resolver": "exception",
        "field": "className"
    },
    "stack_trace": {
        "$resolver": "exception",
        "field": "stackTrace",
        "stackTrace": {
            "stringified": true
        }
    },
    "@timestamp": {
        "$resolver": "timestamp",
        "pattern": {
            "format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'",
            "timeZone": "UTC",
            "locale": "en_US"
        }
    }
}
